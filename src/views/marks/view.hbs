{{>navbar}}

{{>sidebar}}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <div class="container-full">
        <!-- Content Header (Page header) -->
        <div class="content-header">
            <div class="d-flex align-items-center">
                <div class="me-auto">
                    <h3 class="page-title">نتیجہ کارڈ</h3>
                </div>

            </div>
        </div>

        <!-- Main content -->
        <section class="invoice printableArea col-8">
            <p class="m-0 fs-20 text-center font-nastaleeq fw-bold">حاصل کردہ نمبرات کی تفصیل</p>
            <div class="row">
                <div class="col-12">
                    <div>
                        <h2 class="d-inline"><span class="fs-16">سال:{{student.session}}</span></h2>
                        <div class="pull-right text-end">
                            <h3 class="fs-16">امتحان: {{student.admission.name}}</h3>
                        </div>
                    </div>
                </div>
                <!-- /.col -->
            </div>
            <div class="row invoice-info">
                <!-- /.col -->
                <div class="col-sm-12 invoice-col mb-15">
                    <div class="invoice-details row no-margin justify-content-between">
                        <div class="col-auto eng"><b class="font-nastaleeq ps-2">رول نمبر: </b>{{student.rollNumber}}
                        </div>
                        <div class="col-auto"><b class="font-nastaleeq ps-2">نام:</b> {{student.name}}</div>
                        <div class="col-auto"><b class="font-nastaleeq ps-2">ولدیت:</b> {{student.fatherName}}</div>
                        <div class="col-auto eng"><b class="font-nastaleeq ps-2">تاریخ پیدائش:</b> {{FormatDate
                            student.dob}}</div>
                    </div>
                </div>
                <!-- /.col -->
            </div>
            <div class="row">
                <div class="col-12 table-responsive">
                    <table class="table table-bordered">
                        <tbody>
                            <tr>
                                <th width="50px">#</th>
                                <th>مضمون</th>
                                <th width="120px">کل نمبر</th>
                                <th width="120px">حاصل کردہ نمبر</th>
                            </tr>
                            {{#each classData.subjects}}
                            <tr>
                                <td>{{increment @index}}</td>
                                <td>{{this.name}}</td>
                                <td class="font-arial">{{this.totalMarks}}</td>
                                <td class="font-arial fw-bold">{{getMarkBySubject this ../student}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                    <div class="row invoice-info">
                        <!-- /.col -->
                        <div class="col-sm-12 invoice-col mb-15">
                            <div class="invoice-details row no-margin justify-content-end">
                                <div class="col-auto font-arial"><b class="ps-2 font-nastaleeq">کل نمبر:
                                    </b>{{student.totalMarks}}</div>
                                <div class="col-auto font-arial"><b class="ps-2 font-nastaleeq">فیصد نمبر:
                                    </b>%{{student.percentage}}</div>
                                <div class="col-auto font-arial"><b class="ps-2 font-nastaleeq">مجموعہ نمبرات:
                                    </b>{{student.obtMarks}}</div>
                            </div>
                        </div>
                        <!-- /.col -->
                    </div>
                </div>
                <!-- /.col -->
            </div>
            <div class="col-12">
                <div class="clearFix">
                    <div class="text-end pb-15 eng">
                        <button class="btn btn-success" type="button"> <span><i class="fa fa-print"></i>
                                Save</span>
                        </button>
                        <button id="print2" class="btn btn-warning" type="button"> <span><i class="fa fa-print"></i>
                                Print</span> </button>
                    </div>
                </div>
            </div>
        </section>

